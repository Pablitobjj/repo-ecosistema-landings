{
  "version": 2,
  "routes": [
    { "src": "^/$", "has": [{ "type": "host", "value": "hispan.io" }], "dest": "/landings/hispano_index.html" },
    { "src": "^/about/?$", "has": [{ "type": "host", "value": "hispan.io" }], "dest": "/landings/hispano_about.html" },
    { "src": "^/platform/?$", "has": [{ "type": "host", "value": "hispan.io" }], "dest": "/landings/hispano_platform.html" },
    { "src": "^/insights/?$", "has": [{ "type": "host", "value": "hispan.io" }], "dest": "/landings/hispano_insights.html" },
    { "src": "^/insights-analysis/?$", "has": [{ "type": "host", "value": "hispan.io" }], "dest": "/landings/hispano_insights_analysis.html" },
    { "src": "^/miami-awakening/?$", "has": [{ "type": "host", "value": "hispan.io" }], "dest": "/landings/hispano_Miami_Awakening.html" },
    { "src": "^/whitepaper/?$", "has": [{ "type": "host", "value": "hispan.io" }], "dest": "/landings/hispano_whitepaper.html" },
    { "src": "^/resources-authentic/?$", "has": [{ "type": "host", "value": "hispan.io" }], "dest": "/landings/hispano_resources_authentic.html" },
    { "src": "^/hispanic-latino-latinx/?$", "has": [{ "type": "host", "value": "hispan.io" }], "dest": "/landings/hispano_hispanic_latino_latinx.html" },

    { "src": "^/resources/(.*)$", "has": [{ "type": "host", "value": "hispan.io" }], "dest": "/public/resources/$1" },

    { "src": "^/$", "has": [{ "type": "host", "value": "pixel.hispan.io" }], "dest": "/landings/landing_cb_pixel.html" },
    { "src": "^/$", "has": [{ "type": "host", "value": "network.hispan.io" }], "dest": "/landings/landing_cb_app.html" }
  ]
}
